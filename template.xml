<?xml version="1.0" encoding="utf-8"?>
<template name="TemplateExample" card_type="2" card_thickness="30" delete="no">
  <fonts>
    <font id="1" name="Arial" size="12" bold="no" italic="no" underline="no"/>
    <font id="2" name="Arial" size="14" bold="yes" italic="yes" underline="no"/>
  </fonts>
  <sides>
    <side name="front" orientation="landscape" rotation="0" k_mode="text">
      <print_types>
        <print_type type="mono">
          <text field="firstName" font_id="1" x="100" y="100" width="0" height="0" angle="0" color="0x000000"/>
          <text field="firstName2" font_id="1" x="100" y="100" width="0" height="0" angle="0" color="0x000000"/>
          <text field="lastName" font_id="2" x="120" y="100" width="0" height="0" angle="0" color="0x000000"/>
          <line x1="100" y1="235" x2="260" y2="235" thickness="10" color="0"/>
          <barcode field="qrCode" code="qrCode" x="100" y="270" width="75" height="75" quiet_zone_width="0"/>
        </print_type>
      </print_types>
    </side>
  </sides>
</template>
