{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PrinterConfig": {
    "EnableUsbDiscovery": true,
    "EnableNetworkDiscovery": false,
    "UsbConnectionTimeoutMs": 10000,
    "UsbOperationTimeoutMs": 30000,
    "EnableUsbRetries": true,
    "MaxUsbRetries": 3,
    "UsbRetryDelayMs": 2000
  }
} 